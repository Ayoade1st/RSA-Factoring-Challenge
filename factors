#!/usr/bin/env bash

factorize() {
    local n=$1
    local p=2
    local q
    local result=""

    while (( p * p <= n )); do
        if (( n % p == 0 )); then
            q=$(( n / p ))
            result+=" $p*$q"
        fi
        (( p++ ))
    done

    echo "$result"
}

if [ $# -ne 1 ]; then
    echo "Usage: factors <file>"
    exit 1
fi

while read -r number; do
    result=$(factorize "$number")
    echo "$result"
done < "$1"

